{
   "editor.tokenColorCustomizations": {
      "textMateRules": [
         {
            "scope": ["red.taxon"],
            "settings": {"foreground": "#ec2b1d"}
         }, {
            "scope": ["pink.taxon"],
            "settings": {"foreground": "#ff1267"}
         }, {
            "scope": ["orange.taxon"],
            "settings": {"foreground": "#ff8e08"}
         }, {
            "scope": ["blue.taxon"],
            "settings": {"foreground": "#2cb3e6"}
         }, {
            "scope": ["green.taxon"],
            "settings": {"foreground": "#6ae22e"}
         }, {
            "scope": ["purple.taxon"],
            "settings": {"foreground": "#a370ff"}
         }, {
            "scope": ["yellow.taxon"],
            "settings": {"foreground": "#ffcc66"}
         }, {
            "scope": ["white.taxon"],
            "settings": {"foreground": "#f8f8f2"}
         }, {
            "scope": ["slate.taxon"],
            "settings": {"foreground": "#648185"}
         }, {
            "scope": ["gray.taxon"],
            "settings": {"foreground": "#a39f83"}
         }, {
            "scope": [
               "storage.type.livescript",
               "variable.parameter.livescript"
            ],
            "settings": {
               "fontStyle": ""
            }
         }
      ]
   },
   "[taxon]": {
      "editor.tabSize": 2,
      "editor.insertSpaces": false,
      "editor.wordWrap": "wordWrapColumn",
      "editor.wordWrapColumn": 110
   },
   "material-icon-theme.files.associations": {
      "*.taxon": "tree",
      "OCTOKEN": "key"
   },
   "material-icon-theme.folders.associations": {
      "vscode-ext": "vscode"
   }
}